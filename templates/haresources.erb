<% if defined? @haresource -%>
<% @haresource.each do |node, comment| -%>
<%= node -%> \
  <% comment.each_with_index do |(key, value), index| -%>
  <%= value['resource'] -%><% if value['value'] -%>::<%= value['value'] -%><% if index < (comment.size - 1 ) -%>  \<% end %><% else %> \<% end %>
  <% end -%>
<% end -%>
<% end -%>
