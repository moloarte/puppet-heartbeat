<% if defined? @haresource -%>
<% @haresource.each do |node, array| -%>
<%= node -%> \
  <%- array.each_with_index do |value,index| -%>
    <%- value.each do | k,v | -%>
  <%= k %><%- if v -%>::<%= v -%><% if index < (array.size - 1 ) -%>  \<%- end -%><% end %>
    <%- end -%>
  <%- end -%>
<% end -%>
<% end -%>
